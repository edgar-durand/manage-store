ge: 0, show: +event.target.value})}\r\n                           value={state.show}\r\n                           className=\"col-lg-1 form-control\"\r\n                           type=\"number\" min=\"0\"\r\n                           max={Object.values(filtered).length}/>&nbsp;&nbsp; / {Object.values(filtered).length}\r\n                    <label className=\"col-1\"/>\r\n                    <button onClick={() => state.page > 0 ? setState({...state, page: state.page - 1}) : null}\r\n                            className=\"btn btn-outline-primary \"><i className=\"fa fa-backward\"/> Prev\r\n                    </button>\r\n                    <button onClick={() => state.page < (Object.values(filtered).length / state.show) - 1 ?\r\n                        setState({...state, page: state.page + 1}) : null\r\n                    } className=\"btn btn-outline-primary \">Next <i className=\"fa fa-forward\"/></button>\r\n                    <label className=\"col-1\">Search</label>\r\n                    <input type=\"text\" name=\"searchField\" className=\"form-control\" onChange={(e) => handleChange(e)}/>\r\n                    <label className=\"col-2\">Match top price:</label>\r\n                    <input type=\"checkbox\" name=\"matchControl\" onChange={() => handleMatchControl()}/>\r\n                    <label/> &nbsp;&nbsp; $ &nbsp;&nbsp;\r\n                    <input type=\"number\" min=\"0\" name=\"match\" className=\"form-control col-1\"\r\n                           disabled={match.active === false}\r\n                           onChange={(e) => handleMatch(e)}/>\r\n                </div>\r\n            </div>\r\n            <div className=\"wrapper wrapper-content col-12\">\r\n\r\n                <div className=\"row float-left col-9 animated fadeInRight \">\r\n                    {\r\n                        Object.values(MO).map((x, index) => {\r\n                            return <ProductGrid\r\n                                key={index}\r\n                                img={x.image}\r\n                                name={x.name}\r\n                                price={x.price_cost}\r\n                                category={x.category}\r\n                                description={x.description}\r\n                                addToCart={(id, e) => addToCart(id, e)}\r\n                                productId={x.id}\r\n                            />\r\n                        })}\r\n\r\n\r\n                </div>\r\n                <CartSummary />\r\n\r\n            </div>\r\n\r\n\r\n        </Fragment>\r\n    )\r\n}\r\nexport default GoForShopping","D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\validation\\passwordRequirements.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\LoadingButton.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Status.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Edit\\EditUI.js",["260"],"import React, {useRef, useEffect, useState} from \"react\"\r\nimport send from \"../../js/send\";\r\nimport authHelper from \"../../js/authHelper\";\r\nimport Select from 'react-select'\r\n\r\nconst Edit = ({\r\n                  handleSubmit,\r\n                  _public,\r\n                  Product_category,\r\n                  inStock,\r\n                  price_vent,\r\n                  price_cost,\r\n                  description,\r\n                  name,\r\n                  handleChange,\r\n                  image,\r\n                  handleFile,\r\n                  handlePublic,\r\n                  handleSelect\r\n              }) => {\r\n    const TOKEN = {token: authHelper()}\r\n    const [category, setCategory] = useState([])\r\n    useEffect(() => {\r\n        send(TOKEN, \"/api/category\", \"get\")\r\n            .then(r => setCategory({...r}))\r\n    }, [])\r\n\r\n    const img = useRef('img');\r\n    const file = u