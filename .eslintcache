then(r => setProduct({\r\n                    ...product, product: {...r}, load: false\r\n                }));\r\n        },\r\n        []\r\n    )\r\n\r\n\r\n    const addToCart = () => {\r\n        store.dispatch({\r\n            type: \"ADD_TO_CART\",\r\n            product: product.product\r\n        })\r\n\r\n    }\r\n\r\n    return (\r\n        <DetailUI\r\n            {..._public}\r\n            photo={image}\r\n            producName={name}\r\n            category={category}\r\n            author={`${store.getState().globalState[0].email}`}\r\n            price={price_cost}\r\n            description={description}\r\n            addToCart={() => addToCart()}\r\n            load={product.load}\r\n        />\r\n    )\r\n};\r\nexport default Detail","D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\MyAccount\\MyAccounts.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Categories\\CategoryEdit.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Categories\\CategoryNew.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Categories\\CategoryShow.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\NewAccount\\NewAccountForm.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\Categories\\CategoryList.js",[],"D:\\Proyectos\\nodeExpress\\React-Starting\\e-store\\myProyects\\src\\components\\GoForShopping\\GoForShopping.js",["259"],"import React, {Fragment, useState, useEffect} from \"react\";\r\nimport ProductGrid from \"../ProductGrid\";\r\nimport dataToPages from \"../../js/dataToPages\";\r\nimport store from \"../../store\";\r\nimport CartSummary from \"../CartSummary/CartSummary\";\r\n\r\nconst GoForShopping = () => {\r\n    const [state, setState] = useState({\r\n        total: 0,\r\n        searchField: \"\",\r\n        match: {active: fals